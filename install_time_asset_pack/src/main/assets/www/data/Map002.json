{"id":1,"name":"EV001","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":241,"indent":0,"parameters":[{"name":"Theme2","volume":90,"pitch":60,"pan":0}]},{"code":105,"indent":0,"parameters":[1,false]},{"code":405,"indent":0,"parameters":["In the land of Cotton Kingdom,"]},{"code":405,"indent":0,"parameters":["where the sun always shines and the people are kind,"]},{"code":405,"indent":0,"parameters":["a great evil has arisen."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["A demon lord has come to this world through a "]},{"code":405,"indent":0,"parameters":["disruption in time and space, "]},{"code":405,"indent":0,"parameters":["and he threatens to destroy everything."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["The king and queen of Cotton Kingdom "]},{"code":405,"indent":0,"parameters":["have called upon their bravest heroes "]},{"code":405,"indent":0,"parameters":["to defeat the demon lord."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["Three mighty warriors have answered the call: "]},{"code":405,"indent":0,"parameters":["the swordswoman, the archer, and the wizard."]},{"code":405,"indent":0,"parameters":["These heroes are the kingdom's only hope."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["The heroes set out on their journey,"]},{"code":405,"indent":0,"parameters":["but they soon realize that the demon lord "]},{"code":405,"indent":0,"parameters":["is a powerful foe."]},{"code":405,"indent":0,"parameters":["He has an army of demons at his command,"]},{"code":405,"indent":0,"parameters":["and he is able to control the forces of nature."]},{"code":405,"indent":0,"parameters":["The heroes are pushed to their limits, "]},{"code":405,"indent":0,"parameters":["but they never gave up."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["With the help of an unknown force,"]},{"code":405,"indent":0,"parameters":["the heroes are able to defeat the demon lord."]},{"code":405,"indent":0,"parameters":["The people of Cotton Kingdom rejoice,"]},{"code":405,"indent":0,"parameters":["and the heroes are hailed as saviors."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["But the unknown force is never revealed."]},{"code":405,"indent":0,"parameters":["Some say it was an act of god, "]},{"code":405,"indent":0,"parameters":["while others believe that it was a mysterious being, "]},{"code":405,"indent":0,"parameters":["who helped the heroes without them even knowing it."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["The peace has returned to Cotton Kingdom, "]},{"code":405,"indent":0,"parameters":["and the heroes are celebrated. "]},{"code":405,"indent":0,"parameters":["A grand welcome ceremony is being prepared for them"]},{"code":405,"indent":0,"parameters":["in the capital city."]},{"code":405,"indent":0,"parameters":[""]},{"code":405,"indent":0,"parameters":["But the heroes know that there is still "]},{"code":405,"indent":0,"parameters":["danger in the world,"]},{"code":405,"indent":0,"parameters":["and they vow to always be ready to fight "]},{"code":405,"indent":0,"parameters":["for what is right."]},{"code":242,"indent":0,"parameters":[5]},{"code":201,"indent":0,"parameters":[0,1,16,18,8,0]},{"code":211,"indent":0,"parameters":[1]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":2,"stepAnime":false,"through":false,"trigg